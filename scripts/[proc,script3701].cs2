// 3701
[proc,script3701](component $component0, int $colour1)
if (cc_find($component0, 0) = ^true) {
	cc_settrans(0);
	if (~on_mobile = false) {
		cc_setonmouseover("script3700(true, $component0, $colour1)");
		cc_setonmouseleave("script3700(false, $component0, $colour1)");
	}
}
if (cc_find($component0, 3) = ^true) {
	cc_settrans(0);
	cc_setgraphic("scrollbar_v2,1");
}
if (cc_find($component0, 4) = ^true) {
	cc_setcolour($colour1);
}
